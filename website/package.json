{
  "name": "website",
  "version": "1.0.0",
  "description": "Alumis website",
  "scripts": {
    "clean": "node ./node_modules/rimraf/bin.js ./dist",
    "start:dev": "npm run clean && node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.config.dev.js --history-api-fallback",
    "start:prod": "npm run clean && node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.config.prod.js --history-api-fallback --mode=production",
    "build": "npm run build:prod",
    "build:dev": "npm run clean && node ./node_modules/webpack-cli/bin/cli.js --config webpack.config.dev.js --mode=development",
    "build:prod": "npm run clean && node ./node_modules/webpack-cli/bin/cli.js --config webpack.config.prod.js --mode=production",
    "watch": "npm run clean && node ./node_modules/webpack-cli/bin/cli.js --config webpack.config.dev.js --mode=development --watch",
    "generate:icons": "node ./src/generate-Octicons.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@alumis/ts": "1.0.0",
    "@babel/core": "7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-react-jsx": "^7.7.4",
    "@babel/preset-env": "7.7.6",
    "@babel/preset-typescript": "7.7.4",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.5.0",
    "css-loader": "3.3.2",
    "file-loader": "^5.0.2",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "0.8.0",
    "monaco-editor": "^0.19.0",
    "monaco-editor-webpack-plugin": "^1.8.1",
    "node-sass": "^4.13.0",
    "octicons": "^8.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "3.0.0",
    "precss": "4.0.0",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "terser-webpack-plugin": "^2.3.0",
    "typescript": "^3.7.4",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "3.3.10",
    "workbox-webpack-plugin": "^5.0.0-beta.0",
    "worker-loader": "^2.0.0"
  },
  "devDependencies": {
    "@teamsupercell/typings-for-css-modules-loader": "^2.1.0",
    "webpack-dev-server": "^3.10.1"
  }
}
